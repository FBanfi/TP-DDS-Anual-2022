{{# partial "titulo" }}
    <title>CarbonOrg-Calculator | Registrar Trayecto</title>
{{/partial}}

{{# partial "contenido" }}
    <section class="marcoCuerpo">
        <section class="cuerpo">
            <title>Registrar Tramo</title>
            <form method="POST" action="/trayectos/tramos/nuevo">
               
                <div>
                    <label for="origen">Origen:</label>
                    <input id="origen" type="text" class="validate">
                <div>
                    <label for="calle_origen">Calle:</label>
                    <input id="calle_origen" type="text" class="validate">

                    <label for="altura_origen">Altura:</label>
                    <input id="altura_origen" type="text" class="validate">
                </div>
                </div>
                <div>
                    <label for="destino">Destino:</label>
                    <input id="destino" type="text" class="validate">
                </div>
                <div>
                    <label for="calle_destino">Calle:</label>
                    <input id="calle_destino" type="text" class="validate">

                    <label for="altura_destino">Altura:</label>
                    <input id="altura_destino" type="text" class="validate">
                </div>
                <div>
                    <label for="transporte">Medio de transporte:</label>
                    <input id="transporte" type="text" class="validate">
                </div>
                <input type="button" value="Cargar">
            </form>
        </section>
    </section>
{{/partial}}

{{> layout-miembros.html.hbs }}